    "kotlin": {
      "command": "/home/users/aredridel/Projects/KotlinLanguageServer/server/build/scripts/server",
      "filetypes": ["kotlin"]
    },
    "nix": {
      "command": "nixd",
      "filetypes": ["nix"],
