ignoreUnknown UseKeychain
IgnoreUnknown AddKeysToAgent
UseKeychain yes
AddKeysToAgent yes

Include ~/.ssh/config.local

Host !127.0.0.1
	AddKeysToAgent yes

Host *.theinternetco.net
	User aredridel
	ForwardX11Trusted yes
    ForwardAgent yes
	ControlMaster auto
	ControlPath ~/.ssh/control-%h-%p-%r

Host *.nbtsc.org *.http80.net *.dinhe.net
	User aredridel
    ForwardAgent yes
	ControlMaster auto
	ControlPath ~/.ssh/control-%h-%p-%r
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

Host *.local
    ForwardAgent yes

Host protocol.club
    User aredridel

Host dinhe.net
	HostName dinhe.net
Host dev-*
	ProxyJump tf-m2-bastion.development.thegrommet.com
	IdentityFile ~/.ssh/id_20211007.key
	IdentityFile ~/.ssh/id_20210629

Host prod-*
	ProxyJump tf-m2-bastion.production.thegrommet.com
	IdentityFile ~/.ssh/id_20211007.key
	IdentityFile ~/.ssh/id_20210629

Host *.thegrommet.com
	IdentityFile ~/.ssh/id_20211007.key
	IdentityFile ~/.ssh/id_20210629
